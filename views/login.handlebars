<div class="container" >
    <article class= "row">
        <h5 class="center-align">Login</h5>
            <div>
                <form id="login-form" class="col s12">
                    <div class="col s12">
                        Username:
                        <div class="input-field col s12">
                            <input id= "username-login" type= "text" class= "form-input">
                            <label for= "username-login" class= "form-label"></label>
                        </div>
                    </div>
                    <div class="col s12">
                        Password:
                        <div class="input-field">
                            <input id= "password-login" type= "text" class= "form-input">
                            <label for= "password-login" class= "form-label"></label>
                        </div>
                    </div>
                    <div class= "center-align">
                        <button type="submit" class="login-btn btn waves-effect waves-light btn-small center-align">Login</button>
                    </div>
                    
                </form>
            </div>
    </article>
</div>


<script src="./js/login.js"></script>